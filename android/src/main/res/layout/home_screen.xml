<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/homeScreen_relativeLayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background" >

    <GridLayout
        android:id="@+id/homeScreen_GridLayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:columnCount="10"
        android:orientation="vertical"
        android:rowCount="13" >

        <ImageButton
            android:id="@+id/homeScreen_carButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="right|top"
            android:layout_row="3"
            android:background="@drawable/car" />

        <ImageButton
            android:id="@+id/homeScreen_houseButton"
            style="@style/AppBaseTheme"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="center_horizontal|top"
            android:layout_row="11"
            android:background="@drawable/house" />

        <ImageButton
            android:id="@+id/homeScreen_logOutButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="right|bottom"
            android:layout_row="12"
            android:background="@drawable/logout" />

        <ImageButton
            android:id="@+id/homeScreen_settings"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="left|bottom"
            android:layout_row="12"
            android:background="@drawable/settings" />

        <ImageButton
            android:id="@+id/homeScreen_sailboatButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="right|top"
            android:layout_row="12"
            android:background="@drawable/sailboat" />

        <ImageButton
            android:id="@+id/homeScreen_calendarButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="left|top"
            android:layout_row="3"
            android:background="@drawable/calendar" />

        <ImageButton
            android:id="@+id/homeScreen_spaceshuttleButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="left|top"
            android:layout_row="12"
            android:background="@drawable/spaceshuttle" />

        <ImageButton
            android:id="@+id/homeScreen_coffeeMachineButton"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_column="9"
            android:layout_gravity="center"
            android:layout_row="12"
            android:background="@drawable/coffee" />

    </GridLayout>

</RelativeLayout>